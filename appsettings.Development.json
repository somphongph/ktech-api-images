{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "CORS-Settings": {
        "Allow-Origins": [
            "http://tripgator.local",
			"http://www.tripgator.local",
            "http://*.tripgator.local",
            "http://localhost", 
            "http://localhost:8080"
        ],
        "Allow-Headers": [ "Accept", "Content-Type", "Origin", "Authorization", "cache-control", "x-requested-with" ],
        "Allow-Methods": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ]
    }
}
